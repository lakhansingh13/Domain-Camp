#include<bits/stdc++.h>
using namespace std;

int fib(int n){
    if((n==1)||(n==0)){
        return n;
    }
    else{
        return(fib(n-1)+fib(n-2));
    }
}
int main(){
    int n, i;
    cout<<"Enter the value of n: ";
    cin>>n;
    cout<<"the fibonacci series is: ";
    for(i=0;i<=n;i++){
        cout<<fib(i)<<endl;
    }
    
}
